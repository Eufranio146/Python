distancia = float(input('Digite a distância da Viagem Please: '))
if distancia > 0:
  if distancia > 200:
    preco = distancia * 0.45
    print('Preço total: {:.2f} AOA'.format(preco))
  else:
    preco = distancia * 0.50
    print('Preço total: {:.2f} AOA'.format(preco))
else:
  print('Distância Invalida')
