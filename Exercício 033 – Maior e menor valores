a = int(input('Primeiro valor: '))
b = int(input('Segundo valor: '))
c = int(input('Terceiro valor: '))
menor = a
maior = b
if b < a and b < c:
  menor = b
if c < b and c < a:
  menor = c
if c > a and c > b:
  maior = c
if a > b and a > c:
  maior = a
print('Maior: {}\nMenor: {}'.format(maior, menor))
  
